from org.bukkit.entity import Player

class BannerTextPlugin(PythonPlugin):
    def onEnable(self):
        self.getLogger().info("Msg plugin loaded")
    
    def banner(self,bf,jf,bk,jk,ba,ja,ki):
        if bk or jk:
            szin = "black_wall_banner"
        else:
            szin = "white_wall_banner"
         

    def onCommand(self, sender, command, label, args):
        if isinstance(sender, Player):
            if command.name == "bannertext":
                sender.sendRawMessage("Banner text not implemented. Text: '" + ' '.join(args) + "'")
                return True

